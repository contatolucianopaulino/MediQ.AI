{
  "version": 2,
  "name": "mediq-ai",
  "builds": [
    {
      "src": "server-hybrid.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-hybrid.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
